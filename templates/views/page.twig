{% extends "base.twig" %}

{% block content %}

  <div class="container">

    <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
      <section class="article-content">
        <h1 class="h1 main-title">{{post.title}}</h1>
        <div class="article-body">
          {{post.content}}
        </div>
      </section>
    </article>

  </div>

{% endblock %}
